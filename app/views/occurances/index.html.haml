%h1 My Diary

#container

%ul
  - @occurances.each do |o|
    %li
      = o.event.category.titlecase + ": " + o.event.name
      - if o.event.category == "mood"
        %br
        = "Severity: " + o.severity.to_s
      %br
      = "At: " + o.readable_at


-# TODO: Replace this with data viz
= javascript_include_tag 'http://cdn.jsdelivr.net/d3js/3.5.17/d3.min.js'
= javascript_include_tag 'http://cdn.jsdelivr.net/npm/taucharts@1/build/production/tauCharts.min.js'
= javascript_include_tag 'occurances'